{% extends 'base.html.twig' %}

{% block body %}
    <h1>Emploi</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ emploi.id }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ emploi.image }}</td>
            </tr>
            <tr>
                <th>Poste</th>
                <td>{{ emploi.poste }}</td>
            </tr>
            <tr>
                <th>Lieu</th>
                <td>{{ emploi.lieu }}</td>
            </tr>
            <tr>
                <th>Numero</th>
                <td>{{ emploi.numero }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ emploi.email }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ emploi.description }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('emploi_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('emploi_edit', { 'id': emploi.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
